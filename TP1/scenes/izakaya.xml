<lsf>
    <initials>
        <reference length="1" />
        <!-- axis length; "0" means no axis displayed -->
        <root id="root" />
    </initials>

    <views default="Inicial">
        <perspective id="Inicial" near="0.1" far="500" angle="45">
            <from x="8" y="3" z="11" />
            <to x="2.5" y="1" z="6" />
        </perspective>
        <ortho id="Ortho_EntryView" near="-5" far="100" left="-4" right="4" top="2.1" bottom="-2.1">
            <from x="2.5" y="2" z="10" />
            <to x="2.5" y="2" z="0" />
            <up x="0" y="1" z="0" />
        </ortho>
        <ortho id="Ortho_SideView" near="-5" far="100" left="-5.2" right="5.2" top="2.5" bottom="-2.5">
            <from x="5" y="2" z="5" />
            <to x="0" y="2" z="5" />
            <up x="0" y="1" z="0" />
        </ortho>
        <ortho id="Ortho_TopView" near="-5" far="100" left="-8" right="8" top="5.1" bottom="-5.1">
            <from x="2.5" y="5" z="5" />
            <to x="2.5" y="0" z="5" />
            <up x="0" y="1" z="1" />
        </ortho>
    </views>

    <illumination>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />           <!-- global ambient -->
        <background r="0.1" g="0.7" b="1.0" a="1.0" />        <!-- background color -->
    </illumination>


    <lights>
        <!-- NOTE: this block "light" must be repeated as necessary with different "id". 
        At least one light should be present, and a maximum of eight lights is accepted. -->
        <light id="ceilingLight1">            <!-- light identifier -->
            <enable value="1" />
            <!-- enable/disable -->
            <position x="2.5" y="6" z="0" w="1.0" /><!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0" a="1.0" />                   <!-- ambient component -->
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />             <!-- diffuse component -->
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />            <!-- specular component -->
        </light>

        <light id="ceilingLight2">            <!-- light identifier -->
            <enable value="1" />
            <!-- enable/disable -->
            <position x="2.5" y="6" z="10" w="1.0" /><!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0" a="1.0" />                   <!-- ambient component -->
